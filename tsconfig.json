{
  "compilerOptions": {
  "rootDir": "./src",
  "outDir": "./dist",
  "module": "commonjs",
  "target": "es2016",
  "esModuleInterop": true,
  "allowSyntheticDefaultImports": true,
  "types": [],
  "sourceMap": true,
  "declaration": true,
  "declarationMap": true,
  "noUncheckedIndexedAccess": true,
  "exactOptionalPropertyTypes": true,
  "strict": true,
  "jsx": "react-jsx",
  "isolatedModules": true,
  "noUncheckedSideEffectImports": true,
  "moduleDetection": "force",
  "skipLibCheck": true
}
}
